<Window x:Class="Barcodes.Core.Views.ShellWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Barcodes.Core"
        mc:Ignorable="d"
        xmlns:prism="http://prismlibrary.com/"
        Title="Barcodes" 
        Height="{StaticResource ShellHeight}" 
        Width="{StaticResource ShellWidth}"
        MinHeight="{StaticResource ShellHeight}" 
        MinWidth="{StaticResource ShellWidth}"
        Icon="pack://application:,,,/Barcodes.Core.UI;component/Resources/icon.ico">
    <Grid>
        <ContentControl prism:RegionManager.RegionName="{x:Static local:RegionNames.Core}" />
    </Grid>
</Window>
